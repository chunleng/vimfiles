{
  "mcpServers": {
    "calculator": {
      "args": [
        "mcp-server-calculator"
      ],
      "autoApprove": [
        "calculate"
      ],
      "command": "uvx",
      "env": {
        "ASDF_PYTHON_VERSION": "3.12.11",
        "ASDF_UV_VERSION": "0.10.4"
      }
    },
    "todoist": {
      "autoApprove": [
        "find-activity",
        "find-comments",
        "find-completed-tasks",
        "find-projects",
        "find-tasks",
        "find-tasks-by-date",
        "search"
      ],
      "disabled": false,
      "disabled_resources": [],
      "disabled_tools": [
        "list-workspaces",
        "user-info",
        "update-sections",
        "project-management",
        "manage-assignments",
        "find-sections",
        "find-project-collaborators",
        "fetch",
        "fetch-object",
        "get-overview",
        "delete-object"
      ],
      "headers": {
        "Authorization": "Bearer ${env:TODOIST_API_KEY}"
      },
      "type": "http",
      "url": "https://ai.todoist.net/mcp"
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": true
    },
    "neovim": {
      "disabled": true
    }
  }
}
